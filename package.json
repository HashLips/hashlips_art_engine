  "description": "NFT generator for Personas Of the Metaverse NFT collection. Loosely based on the Hashlips art generator",
  "main": "index.js",
  "bin": "index.js",
  "pkg": {
    "assets": [
      "layers/**/*",
      "node_modules/**/*",
      "src/**/*"
    ]
  },
